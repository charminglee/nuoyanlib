                                "$font_size": "normal",
                                "$is_new_nine_slice": false,
                                "$nine_slice_buttom": 0,
                                "$nine_slice_left": 0,
                                "$nine_slice_right": 0,
                                "$nine_slice_top": 0,
                                "$nineslice_size": [
